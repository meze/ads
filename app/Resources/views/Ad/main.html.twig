{% extends '::base.html.twig' %}
{% block body %}

<table class="table table-bordered table-hover table-condensed">
    <thead>
        <tr>
            <th>Original URL</th>
            <th>Target URL</th>
            <th>Views</th>
            <th>Clicks</th>
            <th>CTR</th>
        </tr>
    </thead>
    {% for ad in ads %}
        <tr>
            <td>{{ ad.site }}</td>
            <td>{{ ad.targetUrl }}</td>
            <td>{{ ad.viewsCount }}</td>
            <td>{{ ad.clicksCount }}</td>
            <td>{{ ad.ctr }}</td>
        </tr>
    {% endfor %}
</table>

{% endblock %}
